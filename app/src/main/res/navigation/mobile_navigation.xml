<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.madcampweek2.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.example.madcampweek2.ui.gallery.RecyclerViewFragment"
        android:label="@string/title_gallery"
        tools:layout="@layout/recycler_view_frag" />
    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.madcampweek2.MapsFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_maps"/>
</navigation>